{
	"name": "taro-virtual-list",
	"version": "1.1.2",
	"description": "taro虚拟列表",
	"main": "dist/index.js",
	"main:h5": "dist/index.esm.js",
	"browser": "dist/index.umd.js",
	"module": "dist/index.esm.js",
	"types": "@types/index.d.ts",
	"source": "src/index.ts",
	"files": [
		"lib",
		"dist",
		"@types"
	],
	"keywords": [
		"taro",
		"taro-virtual-list",
		"taro3-virtual-list",
		"virtual-list",
		"虚拟列表",
		"ui"
	],
	"scripts": {
		"dev": "npm run dev:lib",
		"dev:weapp": "npm run build:weapp -- --watch",
		"dev:h5": "npm run build:h5 -- --watch",
		"dev:lib": "tsc --project ./tsconfig.build.json --watch --incremental",
		"build": "npm run build:rollup && npm run build:lib",
		"build:weapp": "taro build --type weapp",
		"build:h5": "taro build --type h5",
		"build:lib": "tsc --project ./tsconfig.build.json --declaration",
		"build:rollup": "rollup --config ./rollup.config.js",
		"lint": "eslint --ext .js,.tsx,.ts,.jsx src",
		"docs:dev": "vuepress dev docs",
		"docs:build": "vuepress build docs",
		"prepublishOnly": "npm install",
		"prepack": "npm run build",
		"clean": "rimraf .temp dist lib coverage",
		"publish": "npm run build && npm publish",
		"commit": "git-cz",
		"release": "standard-version"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tingyuxuan2302/taro3-virtual-list.git"
	},
	"author": "byang5@trip.com",
	"license": "ISC",
	"devDependencies": {
		"@babel/core": "^7.8.0",
		"@babel/runtime": "^7.7.7",
		"@rollup/plugin-commonjs": "^13.0.0",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^8.0.1",
		"@tarojs/cli": "3.3.2",
		"@tarojs/components": "3.3.2",
		"@tarojs/mini-runner": "3.3.2",
		"@tarojs/react": "3.3.2",
		"@tarojs/runtime": "3.3.2",
		"@tarojs/taro": "3.3.2",
		"@tarojs/webpack-runner": "3.3.2",
		"@types/node": "^14.0.13",
		"@types/react": "^16.8.20",
		"@types/react-dom": "^17.0.0",
		"@typescript-eslint/eslint-plugin": "^4.16.1",
		"@typescript-eslint/parser": "^4.16.1",
		"babel-preset-taro": "3.3.2",
		"commitizen": "^4.2.4",
		"cross-env": "^5.2.1",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^7.18.0",
		"eslint-config-taro": "3.3.2",
		"eslint-plugin-import": "^2.12.0",
		"eslint-plugin-react": "^7.8.2",
		"eslint-plugin-react-hooks": "^4.2.0",
		"husky": "^4.2.3",
		"jest": "26",
		"lint-staged": "^8.0.3",
		"marked": "^1.2.8",
		"prettier": "^2.2.1",
		"prop-types": "^15.7.2",
		"react": "^16.13.0",
		"react-dom": "^16.13.0",
		"rimraf": "3.0.2",
		"rollup": "^2.16.1",
		"rollup-plugin-copy": "^3.3.0",
		"rollup-plugin-css-porter": "^1.0.2",
		"rollup-plugin-scss": "^2.6.1",
		"rollup-plugin-typescript2": "^0.27.1",
		"rollup-plugin-visualizer": "^4.0.4",
		"standard-version": "^9.3.1",
		"stylelint": "13.6.0",
		"stylelint-config-standard": "20.0.0",
		"stylelint-scss": "3.17.2",
		"ts-jest": "26",
		"typescript": "^3.8.3"
	},
	"bugs": {
		"url": "https://github.com/tingyuxuan2302/taro3-virtual-list/issues"
